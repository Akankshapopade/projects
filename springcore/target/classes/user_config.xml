<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">
            <bean class="com.jspiders.springcore.entity.User" name="user1">
            <property name="id" value="1"> </property>
            <property name="userName" value="akanksha"> </property>
            <property name="email" value="akanksha@gmail.com"> </property>
            <property name="password" value="akanksha123"> </property>
            </bean>
            <bean class="com.jspiders.springcore.entity.User" name="user2">
            <property name="id" value="2"> </property>
            <property name="userName" value="ragini"> </property>
            <property name="email" value="ragini@gmail.com"> </property>
            <property name="password" value="ragini123"> </property>
            </bean>
            
             <bean class="com.jspiders.springcore.entity.User" name="user3"> 
            <constructor-arg name="id" value="3"/>
            <constructor-arg name="userName" value="lavu"/>
            <constructor-arg name="email" value="lavu@gmail.com"/>
            <constructor-arg name="password" value="lavu123"/>
            </bean>
            
</beans>